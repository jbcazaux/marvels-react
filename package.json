{
  "name": "marvels-react",
  "version": "1.0.0",
  "description": "Marvels-react for vente-privee",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --progress",
    "build": "webpack --verbose",
    "test": "jest",
    "test:w": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbcazaux/marvels-react.git"
  },
  "author": "jbcazaux",
  "devDependencies": {
    "@types/axios": "0.9.35",
    "@types/enzyme": "2.7.2",
    "@types/es6-shim": "0.31.32",
    "@types/history": "2.0.45",
    "@types/jest": "18.1.1",
    "@types/node": "7.0.5",
    "@types/react": "15.0.6",
    "@types/react-bootstrap": "0.0.44",
    "@types/react-dom": "0.14.22",
    "@types/react-redux": "4.4.36",
    "@types/react-router": "3.0.0",
    "@types/react-router-redux": "4.0.39",
    "@types/redux": "3.6.31",
    "@types/redux-thunk": "2.1.31",
    "clean-webpack-plugin": "0.1.15",
    "enzyme": "2.7.1",
    "es6-shim": "0.35.3",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "jest": "18.1.0",
    "react-addons-test-utils": "15.4.2",
    "source-map-loader": "0.1.6",
    "ts-jest": "18.0.2",
    "ts-loader": "2.0.0",
    "tslint": "4.4.2",
    "typescript": "2.1.5",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.2.1"
  },
  "dependencies": {
    "axios": "0.15.3",
    "react": "15.4.2",
    "react-bootstrap": "0.30.7",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "ts-md5": "1.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "testPathIgnorePatterns": ["/node_modules/", "/__tests__/__mocks__/"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/__tests__/__mocks__/file-mock.ts"
    }
  }
}
